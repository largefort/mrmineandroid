{
  "name": "Mr.Mine",
  "version": "1.0.0",
  "description": "Dig.",
  "main": "main.js",
  "scripts": {
    "start": "set APP_DEV=true && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "",
  "keywords": [
    "Mr.Mine"
  ],
  "author": "Oblouk LLC",
  "license": "",
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-rebuild": "^1.11.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "archiver": "^5.0.2",
    "electron-json-storage": "^4.2.0"
  },
  "build": {
    "win": {
      "asar": false,
      "target": "nsis",
      "icon": "Shared/icon.png"
    },
    "directories": {
      "buildResources": "."
    },
    "files": [
      "*",
      "node_modules/**/*",
      "package.json",
      "Shared",
      "Shared/Audio",
      "Shared/Assets",
      "popups",
      "Shared/Assets/Characters",
      "Shared/Assets/Drill",
      "Shared/Fonts",
      "Shared/Assets/Monsters",
      "Shared/Assets/Relics",
      "Shared/Assets/BackpackItems",
      "Shared/Assets/Tutorial",
      "Shared/Assets/UI",
      "Shared/Assets/World",
      "Core",
      "mods",
      "mods/Zipped",
      "Shared/src",
      "!node_modules.zip",
      "!dist.zip",
      "!build.bat",
      "!Unused",
      "!Deferred",
      "!backend",
      "!backend_stats",
      "!*.php",
      "!*.py",
      "!start.bat"
    ]
  },
  "extraFiles": [
    ".",
    "",
    "*"
  ]
}
